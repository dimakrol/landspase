{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "~5.3",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "aws/aws-sdk-php": "~3.0",
        "predis/predis": "~1.1@dev",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-rackspace": "~1.0",
        "league/flysystem": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "yajra/laravel-datatables-oracle": "~6.0",
        "laravelcollective/html": "5.3.*",
        "laravelbook/ardent": "^3.4",
        "cviebrock/laravel-elasticsearch": "^1.0",
        "filp/whoops": "^2.1",
        "intervention/image": "^2.3",
        "nwidart/laravel-modules": "^0.10.0",
        "barryvdh/laravel-debugbar": "^2.2",
        "laravie/html": "5.3.*",
        "orchestra/asset": "3.3.*",
        "laravel/scout": "^1.0",
        "tamayo/laravel-scout-elastic": "dev-master",
        "algolia/algoliasearch-client-php": "^1.10"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "files": [
            "app/Helpers/User.php",
            "app/Helpers/Geo.php",
            "app/Helpers/Setting.php",
            "app/Helpers/View.php",
            "app/Helpers/Debug.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
